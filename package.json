{
  "name": "rgi-reviews-module",
  "description": "A module seeking to hew as closely to REI's User Product Reviews as possible",
  "author": "Sam Quady",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "build": "webpack",
    "start": "node server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:client": "webpack --watch",
